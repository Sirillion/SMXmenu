		</append>


<!--	// SMX // Widget Button -->

		<append xpath="/controls">
			<smxmenu_widgetbutton 	depth="3"
									pos="0,0"
									width="200"
									height="32"
									pivot="left"
									caption=""
									caption_key=""
									enabled_font_color="[labelColor]"
									disabled_font_color="[disabledLabelColor]"
									tooltip_key=""
