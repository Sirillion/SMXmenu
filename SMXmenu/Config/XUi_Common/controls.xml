						disabled_font_color="${disabled_font_color}" 
						controller="SimpleButton" 
						visible="${visible}" 
						button_enabled="${enabled}">

					<button	name="clickable" 
							pos="10,${Round(-height/2,0)}" 
							sprite="smxlib_button_background"
							defaultcolor="[smxmenu_element_button_foreground]"
							hoversprite="smxlib_button_foreground"
							hovercolor="[smxmenu_element_button_hover]"
