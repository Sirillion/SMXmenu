<SMXmenu name="XUi_Common/controls.xml">

<!--	// SMX // Main Menu Button -->

		<append xpath="/controls">
			<smxmenu_mainmenubutton	depth="3"
									pos="0,0"
									width="200"
									height="32"
									pivot="left"
									caption=""
									caption_key=""
									enabled_font_color="[labelColor]"
									disabled_font_color="[disabledLabelColor]"
									tooltip_key=""
									visible="true"
									enabled="true"
									sound="[default_click]">

				<rect	name="${name}"
						depth="${depth}"
						pos="${pos}"
						width="${width}"
						height="${height}"
						font_size_default="${height-32}"
						enabled_font_color="${enabled_font_color}"
						disabled_font_color="${disabled_font_color}"
						controller="SimpleButton"
						visible="${visible}"
						button_enabled="${enabled}">

					<button	name="clickable"
							pos="10,${Round(-height/2,0)}"
							sprite="smxlib_button_background"
							defaultcolor="[smxmenu_element_button_foreground]"
							selectedcolor="[smxmenu_element_button_selected]"
							hoversprite="smxlib_button_foreground"
							hovercolor="[smxmenu_element_button_hover]"
							hoverscale="1.1"
							pivot="${pivot}"
							tooltip_key="${tooltip_key}"
							sound_play_on_hover="[default_hover]"
							sound="${sound}" />

					<label		name="btnLabel"
								depth="2"
								pos="10,${Round(-2-height/2,0)}"
								width="${width-40}"
								height="${height-33}"
								style="smxcore_label_normal"
								text="${caption}"
								text_key="${caption_key}"
								font_size="${height-34}"
								upper_case="true"
								pivot="${pivot}"
								justify="center" />
				</rect>
			</smxmenu_mainmenubutton>
		</append>


<!--	// SMX // Options Default Button -->

		<append xpath="/controls">
			<smxmenu_defaultbutton	depth="3"
									pos="0,0"
									width="30"
									height="30"
									enabled_font_color="[labelColor]" 
									disabled_font_color="[disabledLabelColor]" 
									visible="true" 
									enabled="true" 
									pivot="center"
									defaultsprite="smxlib_ui_game_symbol_default" 
									defaultcolor="[smxmenu_element_accentcolor_2]" 
									hoversprite="smxlib_ui_game_symbol_default" 
									hovercolor="[white]" 
									selectedsprite="smxlib_ui_game_symbol_default" 
									selectedcolor="[mediumGrey]"
									foregroundlayer="true" 
									sound="[default_click]" 
									justify="center" 
									caption="" 
									tooltip_key="${tooltip_key}" 
									font_size="28" 
									upper_case="false" 
									parse_actions="false" >

				<rect	name="${name}" 
						depth="${depth}" 
						pos="${pos}" 
						width="${width}" 
						height="${height}" 
						enabled_font_color="${enabled_font_color}" 
						disabled_font_color="${disabled_font_color}" 
						controller="SimpleButton" 
						visible="${visible}" 
						button_enabled="${enabled}">

					<!--sprite	depth="1" 
							name="border" 
							pos="0,${Round(-height/2,0)}" 
							color="[limeGreen]" 
							type="sliced" 
							foregroundlayer="true" 
							pivot="center" /-->

					<button	name="clickable" 
							depth="99"
							size="30,30"
							pos="0,${Round(-height/2,0)}" 
							sprite="${defaultsprite}" 
							defaultcolor="${defaultcolor}" 
							disabledcolor="[darkGrey]" 
							hoversprite="${hoversprite}" 
							hovercolor="${hovercolor}" 
							selectedsprite="${selectedsprite}" 
							selectedcolor="${selectedcolor}" 
							foregroundlayer="${foregroundlayer}" 
							type="sliced" 
							hoverscale="1.0" 
							pivot="${pivot}" 
							tooltip_key="${tooltip_key}" 
							sound_play_on_hover="[default_hover]" 
							sound="${sound}" /> 
					
					<label	name="btnLabel" 
							depth="2"
							pos="0,${Round(-height/2,0)}" 
 							width="${width}" 
							height="${height-2}" 
							style="smxcore_label_normal"
							text="" 
							text_key="" 
							font_size="${font_size}" 
							pivot="${pivot}" 
							justify="${justify}" 
							upper_case="${upper_case}" 
							parse_actions="${parse_actions}" />
				</rect>
			</smxmenu_defaultbutton>
		</append>


<!--	// SMX // Credits Button -->

		<append xpath="/controls">
			<smxmenu_tfpcreditsbutton 	depth="3"
										pos="0,0"
										width="200"
										height="32"
										pivot="left"
										caption=""
										caption_key=""
										enabled_font_color="[labelColor]"
										disabled_font_color="[disabledLabelColor]"
										tooltip_key=""
										visible="true"
										enabled="true"
										sound="[default_click]">

				<rect	name="${name}"
						depth="${depth}"
						pos="${pos}"
						width="${width}" 
						height="${height}" 
						enabled_font_color="${enabled_font_color}" 
						disabled_font_color="${disabled_font_color}" 
						controller="SimpleButton" 
						visible="${visible}" 
						button_enabled="${enabled}">

					<button	name="clickable"
							pos="10,${Round(-height/2,0)}"
							sprite="smxmenu_mainmenu_tfp_logo"
							hovercolor="[smxmenu_element_button_hover]"
							hoverscale="1.1"
							pivot="${pivot}"
							tooltip_key="${tooltip_key}"
							sound_play_on_hover="[default_hover]"
							sound="${sound}" />

					<label	name="btnLabel"
							depth="2"
							pos="10,${Round(-height/2,0)}"
							width="${width-20}"
							style="smxcore_label_normal"
							text="${caption}"
							text_key="" 
							font_size="${height-26}"
