<!--	// SMX // Video/Audio/Controls Option Entries -->

		<append xpath="/controls">
			<smxmenu_settings	 	depth="6" 
									pos="0,0"
									width="1130"
									height="75"
									values="@DEF" 
									value_min="@DEF" 
									value_max="@DEF" 
									value_wrap="@DEF" 
									value_increment="@DEF" 
									reverse_list="@DEF" 
									localization_prefix="" 
									format_string=""
									visible="true">
				<rect	pos="${pos}" 
						width="${width}" 
						height="75"
						visible="${visible}">

					<sprite	name="background"
							depth="13" 
							pos="-20,0" 
							sprite="smxhud_chat_background" 
							color="[white]" 
							globalopacitymod="0.4"
							visible="${visible}" />

					<label	name="title"
							depth="14" 
							pos="0,-4" 
							width="800" 
							height="26" 
							style="hover, smxcore_label_normal" 
							text_key="${text_key}" 
							font_size="22" 
							color="[smxmenu_element_labelcolor_1]" 
							pivot="topleft" 
							justify="left" 
							tooltip_key="${tooltip_key}" 
							visible="${visible}" />

					<label	name="description"
							depth="14" 
							pos="0,-32" 
							width="800" 
							height="60" 
							style="hover, smxcore_label_normal" 
							text_key="${tooltip_key}" 
							font_size="18" 
							color="[smxmenu_element_labelcolor_2]" 
							pivot="topleft" 
							justify="left" 
							visible="${visible}" />

					<smxmenu_combobox	name="${name}" 
										depth="15"
										pos="800,-23" 
										width="300" 
										height="32" 
										type="${type}" 
										values="${values}" 
										value_min="${value_min}" 
										value_max="${value_max}" 
										value_wrap="${value_wrap}" 
										value_increment="${value_increment}" 
										reverse_list="${reverse_list}" 
										localization_prefix="${localization_prefix}" 
										format_string="${format_string}" 
										tooltip_key="${tooltip_key}" 
										visible="${visible}" />
				</rect>
			</smxmenu_settings>
		</append>


<!--	// SMX // Video/Audio/Controls Option Entries Combobox Design -->

		<append xpath="/controls">
			<smxmenu_combobox	depth="3" 
								pos="0,0" 
								width="200" 
								height="30"
								value_min="@DEF" 
								value_max="@DEF" 
								value_wrap="@DEF" 
								value_increment="@DEF" 
								values="@DEF" 
								reverse_list="@DEF" 
								localization_prefix="" 
								format_string="" 
								enabled_color="[white]" 
								disabled_color="[lightGrey]" 
								visible="true"
								tooltip_key="" 
								foregroundlayer="false" >

				<rect	name="${name}" 
						controller="${type}" 
						depth="${depth}" 
						pos="${pos}" 
						width="${width}" 
						height="${height}" 
						value_min="${value_min}" 
						value_max="${value_max}" 
						value_wrap="${value_wrap}" 
						value_increment="${value_increment}" 
						values="${values}" 
						reverse_list="${reverse_list}" 
						localization_prefix="${localization_prefix}" 
						format_string="${format_string}" 
						enabled_color="${enabled_color}" 
						disabled_color="${disabled_color}" 
						pivot="left" 
						visible="${visible}">

					<button	name="back"
							depth="${1+depth}" 
							style="icon30px, press, hover" 
							pos="${height/2},${-height/2}" 
							width="${height}" 
							height="${height}" 
							pivot="center" 
							sprite="ui_game_symbol_arrow_left" 
							disabledcolor="[disabledColor]" 
							sound="[paging_click]" 
							tooltip_key="${tooltip_key}" />

					<sprite	name="background"  
							depth="${1+depth}" 
							style="icon30px" 
							pos="${2+height},0" 
							width="${width-height*2-4}" 
							height="${height}" 
							pivot="topleft" 
							color="[black]" 
							type="sliced" 
							foregroundlayer="${foregroundlayer}" />

					<sprite	name="backgroundbar"  
							depth="${1+depth}" 
							style="icon30px" 
							pos="${2+height},${-height+3}" 
							width="${width-height*2-4}" 
							height="3" 
							pivot="topleft" 
							sprite="smxmenu_settings_combobox_bar"
							color="[black]" 
							type="sliced" 
							visible="{isnumber}" 
							foregroundlayer="${foregroundlayer}" />

					<sprite	name="fill"
							depth="${2+depth}" 
							pos="${2+height},${-height+3}" 
							width="${width-height*2-4}" 
							height="3" 
							pivot="topleft" 
							sprite="smxmenu_settings_combobox_bar"
							color="[white]" 
							type="filled"
							fill="{fillvalue}" 
							filldirection="horizontal" 
							visible="{isnumber}" 
							foregroundlayer="true" />

					<label	name="currentValue"
							depth="${3+depth}" 
							pos="${2+height},${-height/2}" 
							width="${width-height*2-4}" 
							height="${height-2}" 
							style="smxcore_label_normal"
							text="" 
							font_size="22" 
							pivot="left" 
							justify="center" 
							color="[smxmenu_element_labelcolor_1]" />

					<button	name="forward"
							depth="${1+depth}" 
							style="icon30px, press, hover"
							pos="${width-height/2},${-height/2}" 
							width="${height}" 
							height="${height}" 
							pivot="center" 
							sprite="ui_game_symbol_arrow_right" 
							disabledcolor="[disabledColor]" 
							sound="[paging_click]" 
							tooltip_key="${tooltip_key}" />

					<panel	name="directvalue"
							style="press, scroll, hover, drag" 
							pos="${2+height},0" 
							width="${width-height*2-4}" 
							height="${height}" 
							pivot="topleft" 
							disableautobackground="true" 
							tooltip_key="${tooltip_key}" />

					<rect	name="indexMarkers"
							pos="${2+height},${-height+3}" 
							width="${width-height*2-4}" 
							height="3" 
							visible="true" 
							active_color="[white]" 
							inactive_color="[black]">
								<sprite depth="${2+depth}" width="20" pivot="topleft" color="[white]" type="sliced" foregroundlayer="true" />
								<sprite depth="${2+depth}" width="20" pivot="topleft" color="[white]" type="sliced" foregroundlayer="true" />
								<sprite depth="${2+depth}" width="20" pivot="topleft" color="[white]" type="sliced" foregroundlayer="true" />
								<sprite depth="${2+depth}" width="20" pivot="topleft" color="[white]" type="sliced" foregroundlayer="true" />
								<sprite depth="${2+depth}" width="20" pivot="topleft" color="[white]" type="sliced" foregroundlayer="true" />
								<sprite depth="${2+depth}" width="20" pivot="topleft" color="[white]" type="sliced" foregroundlayer="true" />
								<sprite depth="${2+depth}" width="20" pivot="topleft" color="[white]" type="sliced" foregroundlayer="true" />
								<sprite depth="${2+depth}" width="20" pivot="topleft" color="[white]" type="sliced" foregroundlayer="true" />
								<sprite depth="${2+depth}" width="20" pivot="topleft" color="[white]" type="sliced" foregroundlayer="true" />
								<sprite depth="${2+depth}" width="20" pivot="topleft" color="[white]" type="sliced" foregroundlayer="true" />
					</rect>
				</rect>
			</smxmenu_combobox>
		</append>


